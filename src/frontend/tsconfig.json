{
  // "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./build",
    "lib": ["ESNext", "dom"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
